    <xs:complexType name="EstimateDocuments">
        <xs:sequence>
            <xs:element name="Document" type="EstimateDocument" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="EstimateDocument">
        <xs:sequence>
            <xs:element name="ContentType" type="ContentType" minOccurs="1" maxOccurs="1"/>
            <xs:choice>
                <xs:element name="BinaryContent" type="BinaryContent"/>
                <xs:element name="FileName" type="StringConstraint255"/>
            </xs:choice>
            <xs:element name="OriginalFileName" type="StringConstraint255" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Response">
        <xs:sequence>
            <xs:element name="status" type="StatusCode"/>
            <xs:element name="errorCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ErrorMessage" type="xs:string" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="PersonName">
        <xs:sequence>
            <xs:element name="FirstName" type="xs:string" minOccurs="0"/>
            <xs:element name="LastName" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!--   Arb-related types  -->
    <xs:complexType name="Injured">
        <xs:annotation>
            <xs:documentation>Type for information about a party's injured party</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FirstName" type="StringConstraint25" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>First name of injured party</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LastName" type="StringConstraint30" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Last name of injured party</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PartyStatus" type="PartyStatusType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Status of injured party</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="PartyStatusType">
        <xs:annotation>
            <xs:documentation>Type describing the injured party</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Driver"/>
            <xs:enumeration value="Occupant"/>
            <xs:enumeration value="Pedestrian"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ProgramType">
        <xs:annotation>
            <xs:documentation>Type to describe the AF program under which a case was docketed</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Automobile"/>
            <xs:enumeration value="Medpay"/>
            <xs:enumeration value="PIP"/>
            <xs:enumeration value="Property"/>
            <xs:enumeration value="Special"/>
            <xs:enumeration value="UninsuredMotorist"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="Evidence">
        <xs:annotation>
            <xs:documentation>Type for all evidence information for a party</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="EvidenceItem" type="EvidenceItem" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="EvidenceItem">
        <xs:annotation>
            <xs:documentation>Type for a specific evidence item</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Type" type="DocumentType" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Type of evidence item</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Description" type="StringConstraint2000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Description of evidence item</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Damage">
        <xs:annotation>
            <xs:documentation>Type for a specific damage item</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Type" type="DamageType" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Type of Damage</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Description" type="StringConstraint4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Description of Damage</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="Money" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Dollar Amount of Damage</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Damages">
        <xs:annotation>
            <xs:documentation>Type for all damage items for a case</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Damage" type="Damage" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TRSDamages">
        <xs:annotation>
            <xs:documentation>Type for all damage items for a case</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Damage" type="TRSDamage" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TRSDamage">
        <xs:sequence>
            <xs:element name="Type" type="TRSDamageType" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Type of Damage</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Description" type="StringConstraint4000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Description of Damage</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Amount" type="Money" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Dollar Amount of Damage</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Company">
        <xs:annotation>
            <xs:documentation>Type for company and subsidiary information</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CoCode" type="Cocode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>AF member number of group company for the party</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Name" type="StringConstraint40" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Name of Group Company associated with cocode.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Subsidiary" type="Subsidiary" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Subsidiary information for the company</xs:documentation>
                </xs:annotation>
            </xs:element>
